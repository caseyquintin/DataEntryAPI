git config --global alias.sync-scripts '!f() { \
    current_branch=$(git rev-parse --abbrev-ref HEAD); \
    git add scripts.js && \
    git commit -m "Update scripts.js" && \
    git checkout main && \
    git checkout $current_branch -- scripts.js && \
    git add scripts.js && \
    git commit -m "Sync scripts.js from $current_branch" && \
    git push origin main && \
    git checkout $current_branch; \
}; f'